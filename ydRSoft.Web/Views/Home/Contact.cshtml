@{
    Layout = null;
}

<canvas id="miCanvas" width="400" height="400"></canvas>

<script>
    function dibujarCuadrado(x, y, lado) {
        var canvas = document.getElementById('miCanvas');
        if (canvas.getContext) {
            var ctx = canvas.getContext('2d');
            ctx.clearRect(0, 0, canvas.width, canvas.height);  // Limpiar el canvas

            // Dibujar el centro azul
            ctx.fillStyle = 'blue';
            ctx.fillRect(x, y, lado, lado);  // Dibuja el cuadrado relleno

            
        }
    }

    // Llamada a la función para dibujar un cuadrado
    dibujarCuadrado(0, 0, 100);
</script>

